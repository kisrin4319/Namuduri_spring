<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC " - //mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
	
	<!-- Member -->
	<typeAlias type="com.spring.member.MemberModel" alias="MemberModel" />
	<typeAlias type="com.spring.member.ZipcodeModel" alias="ZipcodeModel"/>
	
	<!-- Admin -->
	
	
	
	<!-- Basket -->	
	<typeAlias type="com.spring.basket.BasketModel" alias="BasketModel" />
	
	<!-- Book -->	
	<typeAlias type="com.spring.book.BooksModel" alias="BooksModel" />
	<typeAlias type="com.spring.book.ReviewModel" alias="ReviewModel" />
	<typeAlias type="com.spring.usedbook.UsedBooksModel" alias="UsedBooksModel"/>
	
	<!-- Order -->
	<typeAlias type="com.spring.order.OrderModel" alias="OrderModel" />
	<typeAlias type="com.spring.order.OrderDetailModel" alias="OrderDetailModel" />
	
	<!-- Board -->
	<typeAlias type="com.spring.board.BoardModel" alias="BoardModel"/>
	
	<!-- Faq -->
	<typeAlias type="com.spring.faq.FaqModel" alias="FaqModel"/>
	
	<!-- WishList -->
	<typeAlias type="com.spring.wishlist.WishModel" alias="WishModel" />
	
	<!-- EFeelog -->
	<typeAlias type="com.spring.efeelog.EFeelogModel" alias="EFeelogModel"/>
	
	<!-- Coupon -->
	<typeAlias type="com.spring.coupon.CouponModel" alias="CouponModel"/>
	</typeAliases>
	
	<mappers>	
		<mapper resource="com/spring/config/adminSQL.xml" />
		<mapper resource="com/spring/config/memberSQL.xml" />
		<mapper resource="com/spring/config/basketSQL.xml" />
		<mapper resource="com/spring/config/bookSQL.xml" />
		<mapper resource="com/spring/config/usedBookSQL.xml"/>
 		<mapper resource="com/spring/config/orderSQL.xml" />
 		<mapper resource="com/spring/config/boardSQL.xml" />
 		<mapper resource="com/spring/config/wishlistSQL.xml" />
 		<mapper resource="com/spring/config/efeelogSQL.xml"/>
 		<mapper resource="com/spring/config/couponSQL.xml"/>
 		<mapper resource="com/spring/config/faqSQL.xml"/>
	</mappers>
</configuration>